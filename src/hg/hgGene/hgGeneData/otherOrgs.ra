# This file contains info to process the homologous genes
# in other species links.

name human
shortLabel Human
tables hgBlastTab 
idSql select target from hgBlastTab where query = '%s'
url ../cgi-bin/hgTracks?org=human&position=%s
hgsid on
priority 10

name mouse
shortLabel Mouse
tables mmBlastTab 
idSql select target from mmBlastTab where query = '%s'
url ../cgi-bin/hgTracks?org=mouse&position=%s
hgsid on
priority 20

name zebrafish
shortLabel Zebrafish
tables drBlastTab
idSql select target from drBlastTab where query = '%s'
url http://www.ensembl.org/Danio_rerio/protview?peptide=%s
priority 30

name drosophila
shortLabel Drosophila
tables dmBlastTab
idSql select target from dmBlastTab where query = '%s'
url http://flybase.bio.indiana.edu/.bin/fbidq.html?%s
priority 40

name cElegans
shortLabel C. elegans
tables ceBlastTab
idSql select target from ceBlastTab where query = '%s'
url http://www.wormbase.org/db/gene/gene?name=%s
priority 50

name yeast
shortLabel Saccharomyces cerevisiae
tables scBlastTab
idSql select target from scBlastTab where query = '%s'
url http://db.yeastgenome.org/cgi-bin/SGD/locus.pl?locus=%s
priority 60

