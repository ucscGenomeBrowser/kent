# Local declaration so that local gold.html is picked up.
track gold override
html gold

# Local declaration so that local gap.html is picked up.
track gap override
visibility hide
html gap

track chainNetMm10 override
visibility pack
chainMinScore 5000
chainLinearGap loose
matrix 16 91,-90,-25,-100,-90,100,-100,-25,-25,-100,100,-90,-100,-25,-90,91

track genscan override
visibility pack

track intronEst override
visibility hide

track mrna override
visibility hide

track xenoRefGene override
visibility pack

searchTable genscan
searchType genePred
termRegex [AGc][ELh][R8][LMX3][012G\.][-0-9\.]+
searchPriority 50

searchTable gold
searchMethod prefix
searchType bed
shortCircuit 1
termRegex [AGJN][CEHL][R68_][X03][0-9]+(\.[12])?
query select chrom,chromStart,chromEnd,frag from %s where frag like '%s%%'
searchPriority 8
