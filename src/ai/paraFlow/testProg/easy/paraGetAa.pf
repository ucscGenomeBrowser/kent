
para getFactorials(int n) into array of int res
    {
    array[n+1] of int r;
    int acc = 1;
    for (int i=1; i<=n; i += 1)
        {
	acc *= i;
	r[i] = acc;
	}
    res = r;
    }

array of int a = (1,2,3,4);
array of array of int manyFac = para (int i in a) get getFactorials(i);
print(a);
print(manyFac);
