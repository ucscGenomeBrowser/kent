# Fly-specific link info.
# This contains info to construct the quick links. 

name genome
tables bdgpGene 
idSql select chrom,txStart+1,txEnd from bdgpGene where name = '%s'

name family
tables bdgpGene
idSql select name from bdgpGene where name = '%s'

name flyBase
shortLabel FlyBase
tables bdgpGeneInfo
idSql select flyBaseId from bdgpGeneInfo where bdgpName = '%s'
preCutAt -
url http://flybase.bio.indiana.edu/.bin/fbidq.html?%s
priority 25

name uniProt
tables bdgpGene
idSql select proteinID from bdgpGene where name = '%s' and proteinId != "n/a"

name refSeq
shortLabel RefSeq
tables bdgpToRefSeq
idSql select value from bdgpToRefSeq where name = '%s'
url http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Search&db=Nucleotide&term=%s&doptcmdl=GenBank&tool=genome.ucsc.edu
priority 40

name pubMed
shortLabel PubMed
tables bdgpSwissProt
idSql select symbol from bdgpSwissProt where bdgpName = '%s' and symbol != SUBSTRING_INDEX(bdgpName,'-',1)
url http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Search&db=PubMed&term=%s&doptcmdl=DocSum&tool=genome.ucsc.edu
priority 60

name geneCards
hide

name stanfordSource
hide

name cgap
hide

name ensembl
url http://www.ensembl.org/Drosophila_melanogaster/geneview?transcript=%s

name aceView
hide
