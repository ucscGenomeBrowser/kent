    filterValues.confidenceLevel 3,2,1,0
    mouseOver <b>Gene name:</b> $geneName<br><b>Panel:</b> $name<br><b>MOI:</b> $modeOfInheritance<br><b>Phenotypes: </b> $phenotypes<br><b>Confidence level:</b> $confidenceLevel
