searchTable wgEncodeGencodeBasicV7
searchMethod prefix
searchType genePred
termRegex ENST[0-9.]+
searchPriority 2.2

searchName wgEncodeGencodeBasicGeneSymV7
searchTable wgEncodeGencodeBasicV7
searchMethod exact
searchType genePred
searchPriority 2.4
query select chrom, txStart, txEnd, name2 from %s where name2 like '%s'

