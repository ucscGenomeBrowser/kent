track recount3
shortLabel recount3
longLabel Recount 3 SRA introns
group rna
type bigBed 9 +
visibility hide
itemRgb on
labelFields none
#maxWindowToDraw 10000000
bigDataUrl /gbdb/$D/recount3/srav1m.bb
filterLabel.strand Strand
filterType.strand multiple
filterValues.strand +,-,.
filterLabel.readcount Filter by supporting split reads
filterByRange.readcount on
filter.readcount 10000:2000000000
filterLimits.readcount 0:2000000000
filterLabel.size Filter by intron size
filterByRange.size on
filter.size 30:100000
filterLabel.sjPair splice junctions (format GT/AG)
filterType.sjPair wildcard
filterText.sjPair *
mouseOver <b>Split read count</b>: $readcount<br><b>Splice donor</b>: $donor<br><b>Splice acceptor</b>: $acceptor<br><b>Intron size</b>: $size bp<br><b>Strand</b>: $strand
iframeUrl https://snaptron.cs.jhu.edu/snaptron-studies/jxn2studies?compilation=srav1m&jid=$$&coords=$S:${-$}
iframeOptions height='300' width='1000' scrolling='yes'

