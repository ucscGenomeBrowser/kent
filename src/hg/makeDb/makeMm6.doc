use strict;

while (my $line = <>) {
    if (substr($line,0,1) ne ">") {
        print $line;
    } else {
        my @fields = split(/\|/, $line);
        my $printed = 0;
        for (my $i = 0; $i < $#fields; $i++) {
                if ($fields[$i] eq "gb" || $fields[$1] eq "dbj") {
                        (my $name, my $vers) = split(/\./,$fields[$i+1]);
