// Convert string to integer

to isnum(byte b) into bit isnum
    {
    string nums = "09";
    isnum = (b >= nums[0] && b <= nums[1]);
    }

to isalpha(byte b) into bit isalpha
    {
    string az = "az";
    string AZ = "AZ";
    isalpha =  (b >= az[0] && b <= az[1] || b >= AZ[0] && b <= AZ[1]);
    }

to stringToInt(string s) into int val
    {
    string zero = "0";
    byte zeroInAscii = zero[0];
    foreach (byte b in s)
        {
	if (!isnum(b))
	    break;
	val *= 10;
	val += b - zeroInAscii;
	}
    }
