    track wgEncodeGencodeV21View2Way 
    shortLabel 2-Way
    view 42-Way 
    visibility hide
    subTrack wgEncodeGencodeV21
    type genePred
    configurable off

        track wgEncodeGencode2wayConsPseudoV21
        subTrack wgEncodeGencodeV21View2Way off
        subGroups view=b2-way name=yTwo-way
        shortLabel 2-way Pseudogenes
        longLabel 2-way Pseudogene Annotation Set from GENCODE Version 21 (Ensembl 77)
        type genePred
        color 255,51,255
        priority 4
    
    track wgEncodeGencodeV21ViewPolya
    shortLabel PolyA
    view cPolya
    visibility hide
    subTrack wgEncodeGencodeV21
    type genePred
    configurable off

        track wgEncodeGencodePolyaV21
        subTrack wgEncodeGencodeV21ViewPolya off
        subGroups view=cPolya name=zPolyA
        shortLabel PolyA
        longLabel PolyA Transcript Annotation Set from GENCODE Version 21 (Ensembl 77)
        type genePred
        color 0,0,0
        priority 5
    
