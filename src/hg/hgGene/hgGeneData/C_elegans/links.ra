# Worm-specific link info.
# This contains info to construct the quick links. 

name genome
tables sangerGene 
idSql select chrom,txStart+1,txEnd from sangerGene where name = '%s'

name family
tables sangerGene
idSql select name from sangerGene where name = '%s'

name wormBase
shortLabel WormBase
tables sangerGene
idSql select name from sangerGene where name = '%s'
url http://www.wormbase.org/db/gene/gene?name=%s
priority 10

name palTrack
shortLabel Protein FASTA
tables sangerGene
idSql select chrom,txStart+1,txEnd,name from sangerGene where name = '%s'
url ../cgi-bin/hgPal?g=sangerGene&c=%s&l=%s&r=%s&i=%s
hgsid on
dbInUrl on
priority 1

name uniProt
tables sangerLinks
idSql select protName from sangerLinks where orfName = '%s'
nameSql select protName from sangerLinks where orfName = '%s'

name refSeq
shortLabel RefSeq
tables sangerToRefSeq
idSql select value from sangerToRefSeq where name = '%s'
url http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Search&db=Nucleotide&term=%s&doptcmdl=GenBank&tool=genome.ucsc.edu
priority 10

name locusLink
hide

name genBank
hide

name pubMed
shortLabel PubMed
tables orfToGene
idSql select value from orfToGene where name = '%s'
url http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Search&db=PubMed&term=%s&doptcmdl=DocSum&tool=genome.ucsc.edu
priority 10

name geneCards
hide

name stanfordSource
hide

name cgap
hide

name ensembl
url http://www.ensembl.org/Caenorhabditis_elegans/geneview?transcript=%s

name aceView
shortLabel AceView
tables kgXref
idSql select geneSymbol from kgXref where kgID = '%s'
priority 10
url http://www.ncbi.nlm.nih.gov/IEB/Research/Acembly/av.cgi?db=worm&l=%s
