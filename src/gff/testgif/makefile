s: s.c common.o dnautil.o memgfx.o gifwrite.o gifcomp.o
	cc s.c common.o dnautil.o memgfx.o gifwrite.o gifcomp.o
	mv a.out s

common.o: common.c
	cc -c common.c
dnautil.o: dnautil.c
	cc -c dnautil.c
memgfx.o: memgfx.c
	cc -c memgfx.c
gifwrite.o: gifwrite.c
	cc -c gifwrite.c
gifcomp.o: gifcomp.c
	cc -c gifcomp.c
