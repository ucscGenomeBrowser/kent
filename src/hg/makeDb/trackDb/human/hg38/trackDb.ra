# Local declaration so that local gold.html is picked up.
track gold override
html gold

searchName assemblyAcc
searchTable gold
searchMethod prefix
query select chrom,chromStart,chromEnd,frag from %s where frag like '%s%%'
searchPriority 5
shortCircuit 1
termRegex [A-Z][A-Z0-9]+(\.[0-9]+)?

# Local declaration so that local gap.html is picked up.
track gap override
html gap


track rmskCM
shortLabel RM crossMatch
longLabel Repeating Elements by RepeatMasker - cross-match alignment engine
group varRep
priority 2149.1
visibility dense
spectrum on
type rmsk
maxWindowToDraw 10000000
canPack off

track rmskBlastn
shortLabel RM rmblastn
longLabel Repeating Elements by RepeatMasker - rmblastn alignment engine
group varRep
priority 3149.1
visibility dense
spectrum on
type rmsk
maxWindowToDraw 10000000
canPack off

