# Mouse-specific link info.

name jax
shortLabel Jackson Lab
tables kgXref
idSql select uniProt.extDbRef.extAcc1 from uniProt.extDbRef,uniProt.extDb,kgXref where uniProt.extDb.val = 'MGD' and uniProt.extDb.id = uniProt.extDbRef.extDb and uniProt.extDbRef.acc = kgXref.spID and kgXref.kgID = '%s' 
priority 31
url http://www.informatics.jax.org/searches/accession_report.cgi?id=%s

name geneCards
shortLabel GeneCards
tables kgXref
idSql select geneSymbol from kgXref where kgID = '%s'
url http://bioinfo.weizmann.ac.il/cards-bin/cardsearch.pl?search=%s
priority 80

name cgap
url http://cgap.nci.nih.gov/Genes/GeneInfo?ORG=Mm&CID=%s

name reactome
shortLabel Reactome
tables proteome.spReactomeEvent kgXref
idSql select spReactomeId.spID from proteome.spReactomeId,proteome.spVariant,kgXref where kgXref.kgID = '%s' and kgXref.spID=variant and spReactomeId.spID=parent;
url http://www.reactome.org/cgi-bin/link?SOURCE=UniProt&ID=%s&
priority 86

name ensembl
url http://www.ensembl.org/Mus_musculus/geneview?transcript=%s

name geneNetwork
shortLabel GeneNetwork
tables geneNetworkId
idSql select Id from geneNetworkId, knownToRefSeq where Id = value and name='%s'
url http://www.genenetwork.org/cgi-bin/WebQTL.py?cmd=sch&refseq=%s&species=mouse
priority 140

name protBrowser
shortLabel Proteome Browser
tables kgXref
idSql select spID from kgXref where kgID = '%s'
nameSql select spId from kgXref where kgID = '%s'
url ../cgi-bin/pbTracks?proteinID=%s
priority 21

