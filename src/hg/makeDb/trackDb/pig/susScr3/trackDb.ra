# Local declaration so that local gold.html is picked up.
track gold override
html gold

# Local declaration so that local gap.html is picked up.
track gap override
visibility hide
html gap

track chainNetMm10 override
visibility pack

track gc5BaseBw override
visibility full

track genscan override
visibility pack

track refGene override
visibility pack

track xenoRefGene override
visibility dense

searchTable genscan
searchType genePred
termRegex [GJc][HL][r18][MXY1-9][-0-9\.]+
searchPriority 50

searchTable gold
searchMethod prefix
searchType bed
shortCircuit 1
termRegex [AGJN][CEHL][M18_][K0178][0-9]+(\.[1-9])?
query select chrom,chromStart,chromEnd,frag from %s where frag like '%s%%'
searchPriority 8

track animalQtl override
url http://www.animalgenome.org/cgi-bin/QTLdb/SS/qdetails?QTL_ID=$p
urlLabel Animal Genome QTL db:

