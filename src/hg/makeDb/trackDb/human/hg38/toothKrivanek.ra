
track toothKrivanek
superTrack on
shortLabel Tooth Krivanek
longLabel  Tooth (molar) cells from Krivanek et al 2020
visibility hide
group singleCell

    track toothKrivanekTissueCellType
    parent toothKrivanek
    type bigBarChart
    visibility pack
    shortLabel Tooth Krivanek Cells
    longLabel  Tooth (molar) cells from Krivanek et al 2020
    barChartMetric gene/genome
    barChartUnit parts per million
    defaultLabelFields name
    labelFields name,name2
    bigDataUrl /gbdb/$D/bbi/toothKrivanek/facet_usual.bb
    barChartStatsUrl /gbdb/$D/bbi/toothKrivanek/facet_usual.facets
    barChartCategoryUrl /gbdb/$D/bbi/toothKrivanek/facet_usual.categories
    barChartFacets group,cell_class,cell_type
    barChartMerge on
    barChartStretchToItem on

    track toothKrivanekFullDetails
    parent toothKrivanek
    type bigBarChart
    visibility hide
    shortLabel Tooth Krivanek Full
    longLabel  Tooth (molar) from Krivanek et al 2020 detailed
    barChartMetric gene/genome
    barChartUnit parts per million
    defaultLabelFields name
    labelFields name,name2
    bigDataUrl /gbdb/$D/bbi/toothKrivanek/facet_detailed.bb
    barChartStatsUrl /gbdb/$D/bbi/toothKrivanek/facet_detailed.facets
    barChartCategoryUrl /gbdb/$D/bbi/toothKrivanek/facet_detailed.categories
    barChartFacets group,cell_class,cell_type,cell_subtype,seurat_clusters,epilepsy_duration,epilepsy_frequency,age,sex,donor
    barChartMerge on
    maxWindowToDraw 20000000
    barChartStretchToItem on
