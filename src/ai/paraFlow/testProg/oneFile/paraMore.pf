class point {float x,y}
dir of point ptDir = (center@(0,0), ul@(10,1), ur@(10,10), lr@(1,10));
print(ptDir);
para (key@val in ptDir) do
    {
    float x = val.x, y = val.y;
    val = (x*2.0,y*2.0);
    }
print(ptDir);

print(ptDir);
para (pt in ptDir) do
    pt = (pt.y,pt.x);
print(ptDir);

to swapXy(dir of point d)
    {
    para (pt in d) do
	pt = (pt.y,pt.x);
    }
swapXy(ptDir);
print(ptDir);

dir of bit bits = ("TRUE"@1, "FALSE"@0);
print(bits);
para (b in bits) do
    b = !b;
print(bits);

to flip(dir of bit d)
   {
   para (b in d) do
      b = !b;
   }
flip(bits);
print(bits);


