    # Make self mapping table for expression. 
    echo 'create table sgdToSgd ( \
          name varchar(10) not null, \
	  value varchar(10) not null, \
	  PRIMARY KEY(name), \
	  UNIQUE (value));' | hgsql sacCer1
    echo 'select name from sgdGene;' | hgsql -N sacCer1 \
