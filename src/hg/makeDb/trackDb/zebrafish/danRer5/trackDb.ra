# Local declaration so that local gold.html is picked up.
track gold
shortLabel Assembly
longLabel Assembly from Fragments
group map
priority 10
visibility hide
color 150,100,30
altColor 230,170,40
type bed 3 +

track ctgPos2
shortLabel Contigs
longLabel Assembly Contigs
group map
priority 10.1
visibility hide
type ctgPos

# Local declaration so that local gap.html is picked up.
track gap
shortLabel Gap
longLabel Gap Locations
priority 11
group map
visibility dense
type bed 3 +

# Unsplit mRNA track for scaffold-based assembly.
track all_mrna
shortLabel $Organism mRNAs
longLabel $Organism mRNAs from GenBank
group rna
priority 54
visibility pack
spectrum on
type psl .
baseColorUseCds genbank
baseColorUseSequence genbank
baseColorDefault diffCodons
showDiffBasesAllScales .
indelDoubleInsert on
indelQueryInsert on
indelPolyA on

track vegaGene
shortLabel Vega Genes
longLabel Vega v28 Annotations
group genes
priority 37
visibility hide
chromosomes chr1,chr2,chr3,chr4,chr5,chr6,chr7,chr8,chr9,chr10,chr11,chr12,chr13,chr14,chr15,chr16,chr17,chr18,chr19,chr20,chr21,chr22,chr23,chr24,chr25,Zv7_scaffold2498,Zv7_scaffold2509,Zv7_scaffold2516,Zv7_scaffold2572
color 12,12,120
type genePred vegaPep
autoTranslate 0
dataVersion VEGA Release 28
url http://vega.sanger.ac.uk/Danio_rerio/geneview?transcript=$$

track vegaPseudoGene
shortLabel Vega Pseudogenes
longLabel Vega v28 Annotated Pseudogenes 
group genes
priority 37.1
visibility hide
chromosomes chr2,chr3,chr4,chr5,chr9,chr12,chr13,chr18,chr19,chr20,chr22,chr24,Zv7_scaffold2509 
color 12,12,120
type genePred
autoTranslate 0
dataVersion VEGA Release 28
url http://vega.sanger.ac.uk/Danio_rerio/geneview?transcript=$$

# Unsplit EST track for scaffold-based assembly.
track all_est
shortLabel $Organism ESTs
longLabel $Organism ESTs Including Unspliced
group rna
priority 57
visibility hide
spectrum on
intronGap 30
type psl est
baseColorUseSequence genbank
indelDoubleInsert on
indelQueryInsert on
maxItems 300

track affyConsensusAndTargetZonWildType
compositeTrack on
shortLabel Wild Type Array
longLabel Zon Lab Expression data for Wild Type Zebrafish
group regulation
priority 80
visibility hide
type expRatio
expScale 2.0
expStep 0.2
groupings affyZonWildTypeGroups
    
    track affyZonWildType
    subTrack affyConsensusAndTargetZonWildType
    shortLabel WT Array Cons
    longLabel Zon Lab Expression for Wild Type Zfish (Consensus Seq.)
    priority 1
   
    track affyTargetZonWildType
    subTrack affyConsensusAndTargetZonWildType
    shortLabel WT Array Targ
    longLabel Zon Lab Expression for Wild Type Zfish (Target Seq.)
    priority 2

track affyZebrafish
compositeTrack on
shortLabel Affy Zebrafish
longLabel Alignments of Affymetrix Sequences from Zebrafish Chip
group regulation
priority 89
visibility hide
type psl .

    track affyZebrafishConsensus
    subTrack affyZebrafish
    shortLabel Affy Zfish Cons
    longLabel Alignments of Affymetrix Consensus Sequences from Zebrafish Chip
    priority 1
    
    track affyZebrafishTarget
    subTrack affyZebrafish
    shortLabel Affy Zfish Targ
    longLabel Alignments of Affymetrix Target Sequences from Zebrafish Chip
    priority 2 

track rmsk
shortLabel RepeatMasker
longLabel Repeating Elements by RepeatMasker
group varRep
priority 149.1
visibility dense
spectrum on
type rmsk
canPack off

track chainMm9
shortLabel $o_Organism Chain
longLabel $o_Organism ($o_date/$o_db) Chained Alignments
group compGeno
priority 100
visibility hide
color 100,50,0
altColor 255,240,200
matrix 16 91,-90,-25,-100,-90,100,-100,-25,-25,-100,100,-90,-100,-25,-90,91
matrixHeader A, C, G, T
spectrum on
type chain mm9
otherDb mm9

track netMm9
shortLabel $o_Organism Net
longLabel $o_Organism ($o_date/$o_db) Alignment Net
group compGeno
priority 100.1
visibility hide
spectrum on
type netAlign mm9 chainMm9
otherDb mm9

