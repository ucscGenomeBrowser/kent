################################################################
# panPan1 - Bonobo - Pan paniscus - primate - primate - 245
################################################################
track chainNetPanPan1
release alpha
compositeTrack on
shortLabel $o_Organism Chain/Net
longLabel $o_Organism ($o_date), Chain and Net Alignments
subGroup1 view Views chain=Chain net=Net
dragAndDrop subTracks
visibility hide
group compGeno
noInherit on
priority 215.30
color 0,0,0
altColor 255,255,0
type bed 3
sortOrder view=+
matrix 16 90,-330,-236,-356,-330,100,-318,-236,-236,-318,100,-330,-356,-236,-330,90
matrixHeader A, C, G, T
otherDb panPan1
chainMinScore 5000
chainLinearGap medium
spectrum on
html chainNet

    track chainNetPanPan1Viewchain
    release alpha
    shortLabel Chain
    view chain
    visibility pack
    parent chainNetPanPan1
    spectrum on

        track chainPanPan1
        release alpha
        parent chainNetPanPan1Viewchain
        subGroups view=chain
        shortLabel $o_Organism Chain
        longLabel $o_Organism ($o_date) Chained Alignments
        type chain panPan1
        html chainNet

    track chainNetPanPan1Viewnet
    release alpha
    shortLabel Net
    view net
    visibility dense
    parent chainNetPanPan1

        track netPanPan1
        release alpha
        parent chainNetPanPan1Viewnet
        subGroups view=net
        shortLabel $o_Organism Net
        longLabel $o_Organism ($o_date) Alignment Net
        type netAlign panPan1 chainPanPan1
        html chainNet

