to initCap(dir of string ds)
    {
    para (s in ds) do
        {
	dyString dy = s.lower();
	s = s.upper();
	dy[0] = s[0];
	s = dy;
	}
    }
initCap(lastFirst);
print(lastFirst);

to keyStutter(dir of string ds)
    {
    para (key@val in ds) do
        {
	val = key+':'+val;
	}
    }
keyStutter(lastFirst);
print(lastFirst);

para(key@val in lastFirst) do
    {
    val = key + '.' + val;
    }
print(lastFirst);

