track primateChainNet
compositeTrack on
shortLabel Primate Chain/Net
longLabel Primate Genomes, Chain and Net Alignments
subGroup1 view Views chain=Chains net=Nets
subGroup2 species Species aChimp=Chimp bGorilla=Gorilla cOrangutan=Orangutan dGibbon=Gibbon eRhesus=Rhesus fBaboon=Baboon gMarmoset=Marmoset hTarsier=Tarsier iMouse_lemur=Mouse_lemur jBushbaby=Bushbaby
subGroup3 clade Clade aHom=Hominidae bCerco=Cercopithecinae cHaplo=Haplorrhini dStrep=Strepsirrhini
dragAndDrop subTracks
visibility hide
group compGeno
noInherit on
priority 496
color 0,0,0
altColor 255,255,0
type bed 3
chainLinearGap medium
chainMinScore 5000
dimensions dimensionX=clade dimensionY=species 
sortOrder species=+ view=+ clade=+
configurable on
html hg19primateChainNet

    track primateChainNetViewchain
    shortLabel Chains
    view chain
    visibility pack
    subTrack primateChainNet
    spectrum on

        track chainPanTro4
        subTrack primateChainNetViewchain on
        subGroups view=chain species=aChimp clade=aHom
        shortLabel $o_Organism Chain
        longLabel $o_Organism ($o_date) Chained Alignments
        type chain panTro4
        otherDb panTro4

        track chainGorGor3
        subTrack primateChainNetViewchain off
        subGroups view=chain species=bGorilla clade=aHom
        shortLabel $o_Organism Chain
        longLabel $o_Organism ($o_date) Chained Alignments
        type chain gorGor3
        otherDb gorGor3

        track chainPonAbe2
        subTrack primateChainNetViewchain off
        subGroups view=chain species=cOrangutan clade=aHom
        shortLabel $o_Organism Chain
        longLabel $o_Organism ($o_date) Chained Alignments
        type chain ponAbe2
        otherDb ponAbe2

        track chainNomLeu1
        release public
        subTrack primateChainNetViewchain off
        subGroups view=chain species=dGibbon clade=aHom
        shortLabel $o_Organism Chain
        longLabel $o_Organism ($o_date) Chained Alignments
        type chain nomLeu1
        otherDb nomLeu1

        track chainNomLeu3
        release alpha,beta
        subTrack primateChainNetViewchain off
        subGroups view=chain species=dGibbon clade=aHom
        shortLabel $o_Organism Chain
        longLabel $o_Organism ($o_date) Chained Alignments
        type chain nomLeu3
        otherDb nomLeu3


        track chainRheMac3
        subTrack primateChainNetViewchain off
        subGroups view=chain species=eRhesus clade=bCerco
        shortLabel $o_Organism Chain
        longLabel $o_Organism ($o_date) Chained Alignments
        type chain rheMac3
        otherDb rheMac3

        track chainPapHam1
        subTrack primateChainNetViewchain off
        subGroups view=chain species=fBaboon clade=bCerco
        shortLabel $o_Organism Chain
        longLabel $o_Organism ($o_date) Chained Alignments
        type chain papHam1
        otherDb papHam1

        track chainCalJac3
        subTrack primateChainNetViewchain off
        subGroups view=chain species=gMarmoset clade=cHaplo
        shortLabel $o_Organism Chain
        longLabel $o_Organism ($o_date) Chained Alignments
        type chain calJac3
        otherDb calJac3

        track chainTarSyr1
        subTrack primateChainNetViewchain off
        subGroups view=chain species=hTarsier clade=cHaplo
        shortLabel $o_Organism Chain
        longLabel $o_Organism ($o_date) Chained Alignments
