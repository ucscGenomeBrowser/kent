################################################################
# ci3 - Ciona intestinalis - invertebrate - 7719
################################################################
track chainNetCi3
release alpha,beta 
compositeTrack on
shortLabel $o_Organism Chain/Net
longLabel $o_Organism ($o_date), Chain and Net Alignments
subGroup1 view Views chain=Chain net=Net
dragAndDrop subTracks
visibility hide
group compGeno
noInherit on
priority 195.3
color 0,0,0
altColor 255,255,0
type bed 3
sortOrder view=+
matrix 16 91,-114,-31,-123,-114,100,-125,-31,-31,-125,100,-114,-123,-31,-114,91
matrixHeader A, C, G, T
otherDb ci3
chainMinScore 1000
chainLinearGap loose
spectrum on
html chainNet

    track chainNetCi3Viewchain
    release alpha,beta
    shortLabel Chain
    view chain
    visibility pack
    parent chainNetCi3
    spectrum on

        track chainCi3
        release alpha,beta
        parent chainNetCi3Viewchain
        subGroups view=chain
        shortLabel $o_Organism Chain
        longLabel $o_Organism ($o_date) Chained Alignments
        type chain ci3
        html chainNet

    track chainNetCi3Viewnet
    release alpha,beta
    shortLabel Net
    view net
    visibility dense
    parent chainNetCi3

        track netCi3
        release alpha,beta
        parent chainNetCi3Viewnet
        subGroups view=net
        shortLabel $o_Organism Net
        longLabel $o_Organism ($o_date) Alignment Net
        type netAlign ci3 chainCi3
        html chainNet

